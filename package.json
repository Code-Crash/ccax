{
  "name": "ccax",
  "version": "1.0.1",
  "description": "This library will help to assert the parameters, instances, classes, functions, primitive/non-primitive types",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publish": "npm publish --access=public",
    "sanity": "npm run format && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-crash/ccax.git"
  },
  "bugs": {
    "url": "https://github.com/code-crash/ccax/issues"
  },
  "homepage": "https://github.com/code-crash/ccax#readme",
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "assert-validator",
    "assert",
    "validate",
    "validator",
    "assertion",
    "asserter",
    "isString",
    "isNumber",
    "param-validator",
    "ccax",
    "code",
    "crash"
  ],
  "author": "code-crash",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.3"
  }
}
